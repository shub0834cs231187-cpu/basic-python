def print_numbers(n):
    # Base case
    if n == 0:
        return
    # Recursive call first for smaller number
    print_numbers(n - 1)
    # Print after recursive call to get increasing order
    print(n, end=" ")

# Taking input from user
num = int(input("Enter a number: "))

print(f"Numbers from 1 to {num}:")
print_numbers(num)
